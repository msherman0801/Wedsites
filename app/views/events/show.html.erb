
<div class="col-sm-12 col-md-12 well" id="content">
<div class="row">
    <div class="col-md-6">
        <h2 style="margin-bottom: 1em;">Create a new event.</h2>
        <%= render 'form' %>
    </div>
    <div class="col-md-6">
        <h3> Title of Event </h3>
        <h4><%= @event.title %></h4>
        <hr>
        <h3> Description of Event </h3>
        <h4><%= @event.description %></h4>
        <hr>
        <h3> Date of Event </h3>
        <h4><%= @event.date %></h4>
        <hr>
        <h3> Location of Event </h3>
        <h4><%= @event.location %></h4>
        <hr>
        <h3> Attire of Event </h3>
        <h4><%= @event.attire %></h4>
    </div>
</div>
</div>