<%= form_for :user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password, id: "password" %>
    <%= f.submit "login" %>
<% end %>

<%= link_to('Log in with Facebook!', '/auth/facebook') %>

<%= link_to "Sign Up", new_user_path %>